{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/forecast-1",
            "dest": "app/main.py"
        },
        {
            "src": "/forecast-2",
            "dest": "app/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "functions": {
        "app/main.py": {
            "maxDuration": 60
        }
    }
}