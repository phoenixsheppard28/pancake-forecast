{
    "version": 2,
    "functions": {
        "app/main.py": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/forecast-1",
            "dest": "app/main.py"
        },
        {
            "src": "/forecast-2",
            "dest": "app/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ]
}